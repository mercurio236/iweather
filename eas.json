{
  "cli": {
    "version": ">= 5.9.3"
  },
  "build": {
    "preview": {
      "autoIncrement": true,
      "android": {
        "distribution": "internal",
        "autoIncrement": "versionCode"
      },
      "ios": {
        "simulator": true
      }
    },
    "production":{
      "autoIncrement": true,
      "android":{
        "autoIncrement": "versionCode"
      }
    }
  },
  "submit":{
    "preview":{
      "android":{
        "track":"internal",
        "serviceAccountKeyPath": "./google-services.json",
        "releaseStatus": "draft"
      }
    }
  }
}
